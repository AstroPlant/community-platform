{
  "kind": "collectionType",
  "collectionName": "library_medias",
  "info": {
    "name": "LibraryMedia"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "media": {
      "type": "dynamiczone",
      "components": [
        "media-type.link",
        "media-type.file",
        "media-type.article"
      ],
      "required": true,
      "max": 1
    },
    "library_section": {
      "model": "library-section",
      "via": "library_medias"
    },
    "author": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
